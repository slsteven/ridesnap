.modal.fade#coming-soon{role: "dialog", 'aria-labelledby' => "Vehicle Report", 'aria-hidden' => "true"}
  .modal-dialog
    .modal-content
      .modal-header
        = button_tag '&times'.html_safe, class: 'close', 'data-dismiss' => "modal", 'aria-label' => "Close"
        = image_tag 'black_logo.png', class: 'modal-logo'
        %h4.modal-title #{Settings.app.name}
      .modal-body
        %h1.text-center COMING SOON!